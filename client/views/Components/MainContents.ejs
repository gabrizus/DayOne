<div class="main-content">
  <h1 class="head">MainHeader</h1>
  <div>
    <% tasks.forEach(function(o) { %>
    <div class="taskCard">
        <div>
          <h1><%= o.task_name.toUpperCase() %></h1>
          <div class="prio-desc">
          <p class="desc"><%= o.description %></p>
          <div class="prio level<%= o.priority_level %>"></div>
          </div>
          <div class="createdAt">
          <% let date =  o.created_at.toString(); let createdAt = date.slice(0,15) %>
          <%= createdAt %>
          </div>
        <div class="buttons">
          <a href="/deleteTask/<%=o.task_id%>" class="btn neg-btn">Delete</a>
          <a href="/editTask/<%=o.task_id%>" class="btn edit-btn">Edit</a>
        </div>
        </div> 
    </div>
    <% }) %>
  </div>
</div>
